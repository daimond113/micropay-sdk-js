{
    "compilerOptions": {
        "experimentalDecorators": true,
        "target": "es6",
        "module": "CommonJS",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "dist",
        "resolveJsonModule": true,
    }
}